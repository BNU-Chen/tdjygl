                                                            "\r",
                                                            "            //获取用户选中的记录id\r",
                                                            "            //var my_lbl=Ext.getCmp(\"userChoice_lblndzr\");\r",
                                                            "            // var my_ids=my_lbl.text;\r",
                                                            "\r",
                                                            "            //用来获得“推送给全部用户”checkbox是否勾选,以及邮件和短信checkbox是否勾选\r",
                                                            "            var mycheckall=Ext.getCmp('pushall_cboxndzr').getValue();\r",
                                                            "            var mycheckMail=Ext.getCmp('sendMail_cboxndzr').getValue();\r",
                                                            "            var mycheckSMS=Ext.getCmp('sendSMS_cboxndzr').getValue();\r",
                                                            "\r",
                                                            "\r",
                                                            "            //都没有选择，提示用户\r",
                                                            "            if(my_record.length===0 && mycheckall===false){\r",
                                                            "                Ext.Msg.alert('提示','请先选择您要操作的行！');    \r",
                                                            "                return;\r",
                                                            "            }\r",
