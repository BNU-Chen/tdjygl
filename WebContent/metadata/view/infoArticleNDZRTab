                                                            "            else if (my_record.length===0 && mycheckall===true){\r",
                                                            "\r",
                                                            "                //发送邮件\r",
                                                            "                if(mycheckMail===true){              \r",
                                                            "                    Ext.Ajax.request({\r",
                                                            "                        url:'setMailPushTrue',\r",
                                                            "                        params:{\r",
                                                            "                            ids:ids,  //待推送地票Id\r",
                                                            "                            userAdresses: '', //推送目标用户\r",
                                                            "                            chanelid:'4'\r",
                                                            "                        },\r",
                                                            "                        success:function(response){\r",
                                                            "                            var myobj=eval('('+response.responseText+')');\r",
                                                            "                            if(myobj.success===\"true\")\r",
                                                            "                            {\r",
                                                            "                                Ext.Msg.alert('提示','邮件发送成功！');\r",
                                                            "                                win.close();\r",
                                                            "                                m_store_ticket.reload();    \r",
                                                            "                            }\r",
                                                            "                            else\r",
                                                            "                            {\r",
                                                            "                                Ext.Msg.alert('提示','邮件发送失败！');   \r",
                                                            "                            }\r",
                                                            "\r",
                                                            "                        },\r",
                                                            "                        failure:function(){\r",
                                                            "                            Ext.Msg.alert('failed','邮件发送失败！');\r",
                                                            "                        }\r",
                                                            "                    });\r",
                                                            "                }\r",
                                                            "\r",
                                                            "                //发送短信\r",
                                                            "                if(mycheckSMS===true){              \r",
                                                            "                    Ext.Ajax.request({\r",
                                                            "                        url:'setSMSPushTrue',\r",
                                                            "                        params:{\r",
                                                            "                            ids:ids,  //待推送地票Id\r",
                                                            "                            userPhones: '', //推送目标用户\r",
                                                            "                            chanelid:'4'\r",
                                                            "                        },\r",
                                                            "                        success:function(response){\r",
                                                            "                            var myobj=eval('('+response.responseText+')');\r",
                                                            "                            if(myobj.success===\"true\")\r",
                                                            "                            {\r",
                                                            "                                Ext.Msg.alert('提示','短信发送成功！');\r",
                                                            "                                win.close();\r",
                                                            "                                m_store_ticket.reload();    \r",
                                                            "                            }\r",
