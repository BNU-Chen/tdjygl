                                                            "                            Ext.Msg.alert('failed','短信发送失败！');\r",
                                                            "                        }\r",
                                                            "                    });\r",
                                                            "                }\r",
                                                            "            }\r",
                                                            "            //推送给选定用户\r",
                                                            "            else \r",
                                                            "            {\r",
                                                            "                var userAdresses=''; var userPhones='';\r",
                                                            "                for(var i=0;i<my_record.length;i++)\r",
                                                            "                {\r",
                                                            "                    userAdresses+=my_record[i].get(\"email\");\r",
                                                            "                    userPhones+=my_record[i].get(\"phone\");\r",
                                                            "                    if(i<my_record.length-1)\r",
                                                            "                    {userAdresses=userAdresses+\",\";\r",
                                                            "                        userPhones=userPhones+\",\";\r",
                                                            "                    }\r",
                                                            "                }\r",
                                                            "\r",
