                                                            "            var mycheckall=Ext.getCmp('pushall_cboxndzc').getValue();          \r",
                                                            "            var mycheckMail=Ext.getCmp('sendMail_cboxndzc').getValue();\r",
                                                            "            var mycheckSMS=Ext.getCmp('sendSMS_cboxndzc').getValue();\r",
                                                            "\r",
                                                            "\r",
                                                            "            //都没有选择，提示用户\r",
