                                                            "                            chanelid:'3'\r",
                                                            "                        },\r",
                                                            "                        success:function(response){\r",
                                                            "\r",
                                                            "                            var myobj=eval('('+response.responseText+')');\r",
                                                            "                            if(myobj.success===\"true\")\r",
                                                            "                            {\r",
                                                            "                                Ext.Msg.alert('提示','邮件发送成功！');\r",
                                                            "                                win.close();\r",
                                                            "                                m_store_ticket.reload();    \r",
                                                            "                            }\r",
                                                            "                            else\r",
                                                            "                            {\r",
                                                            "                                Ext.Msg.alert('提示','邮件发送失败！');   \r",
                                                            "                            }\r",
                                                            "\r",
                                                            "                        },\r",
                                                            "                        failure:function(){\r",
                                                            "                            Ext.Msg.alert('failed','邮件发送失败！');\r",
                                                            "                        }\r",
                                                            "                    });\r",
                                                            "                }\r",
